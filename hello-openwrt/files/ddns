
config ddns 'global'
	option ddns_dateformat '%F %R'
	option ddns_loglines '250'
	option upd_privateip '0'

config service 'aliyun'
	option service_name 'aliyun.com'
	option enabled '1'
	option username '12345'
	option password '54321'
	option ip_source 'web'
	option ip_url 'http://4.ipw.cn'
	option interface 'wan'
	option use_syslog '0'
	option lookup_host 'cdn-two.517405.xyz'
	option domain 'cdn-two.517405.xyz'

config service 'aliyun_ipv6'
	option use_ipv6 '1'
	option service_name 'aliyun.com'
	option enabled '1'
	option username '12345'
	option password '54321'
	option ip_source 'web'
	option ip_url 'http://6.ipw.cn'
	option interface 'wan'
	option use_syslog '0'
	option lookup_host 'cdn-two.517405.xyz'
	option domain 'cdn-two.517405.xyz'

